<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<submenu></submenu>
	<unique-id>1746475017865603-1</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Example for a custom action</description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>preferences-system-user-sudo</icon>
	<name>Open thunar as root here</name>
	<submenu></submenu>
	<unique-id>1746874936155287-1</unique-id>
	<command>pkexec env DISPLAY=$DISPLAY XAUTHORITY=$XAUTHORITY dbus-launch thunar %f</command>
	<description>Open thunar as root here</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>system-file-manager-root</icon>
	<name>Open folder as root</name>
	<submenu></submenu>
	<unique-id>1493475601060449-3</unique-id>
	<command>pkexec thunar %f</command>
	<description></description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
</action>
